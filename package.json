{
  "name": "telegroam",
  "version": "1.0.0",
  "description": "Send messages and photos to your own Telegram bot and have them appear in the Daily Notes page of your Roam graph.",
  "main": "./build/main.js",
  "scripts": {
    "build": "roamjs-scripts build",
    "prebuild:roam": "npm install",
    "build:roam": "roamjs-scripts build --depot",
