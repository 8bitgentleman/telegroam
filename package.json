    "dev": "roamjs-scripts dev"
  },
  "license": "MIT",
  "devDependencies": {
    "roamjs-scripts": "^0.26.22"
  },
  "dependencies": {
    "roamjs-components": "^0.74.28",
    "cors-anywhere": "^0.4.4"
  }
}